 lrx="1042" lry="1506" ulx="1011" uly="1479" xml:id="zone_dac954ec-c394-11e8-b8a8-f40f243b5e05" /><zone lrx="1050" lry="1506" ulx="1043" uly="1479" xml:id="zone_dac9562e-c394-11e8-9c77-f40f243b5e05" /><zone lrx="1072" lry="1506" ulx="1065" uly="1479" xml:id="zone_dac95776-c394-11e8-8e83-f40f243b5e05" /><zone lrx="1089" lry="1506" ulx="1082" uly="1479" xml:id="zone_dac958c0-c394-11e8-bda9-f40f243b5e05" /><zone lrx="1100" lry="1506" ulx="1093" uly="1479" xml:id="zone_dac95a00-c394-11e8-865b-f40f243b5e05" /></surface></surfaceGrp>
