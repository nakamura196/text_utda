><zone lrx="1546" lry="838" ulx="1506" uly="543" xml:id="zone_22491726-c393-11e8-b2f2-f40f243b5e05" /><zone lrx="1376" lry="1020" ulx="1334" uly="592" xml:id="zone_224917c8-c393-11e8-891a-f40f243b5e05" /><zone lrx="1371" lry="1184" ulx="1330" uly="869" xml:id="zone_224918a4-c393-11e8-ad7f-f40f243b5e05" /><zone lrx="1368" lry="1474" ulx="1330" uly="1389" xml:id="zone_22491958-c393-11e8-b235-f40f243b5e05" /><zone lrx="1241" lry="1551" ulx="1202" uly="1134" xml:id="zone_22491a02-c393-11e8-a1ae-f40f243b5e05" /><zone lrx="971" lry="1237" ulx="927" uly="1110" xml:id="zone_22491aa4-c393-11e8-9552-f40f243b5e05" /><zone lrx="971" lry="1360" ulx="927" uly="1294" xml:id="zone_22491b4c-c393-11e8-b308-f40f243b5e05" /><zone lrx="914" lry="699" ulx="871" uly="583" xml:id="zone_22491c48-c393-11e8-bd7d-f40f243b5e05" /><zone lrx="847" lry="717" ulx="813" uly="565" xml:id="zone_22491cf8-c393-11e8-8771-f40f243b5e05" /></surface></surfaceGrp>
