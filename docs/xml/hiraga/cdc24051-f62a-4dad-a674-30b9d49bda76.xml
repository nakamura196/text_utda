:id="zone_d3131e38-c397-11e8-a2ea-f40f243b5e05" /><zone lrx="1523" lry="1148" ulx="1515" uly="1122" xml:id="zone_d3131eec-c397-11e8-ac0a-f40f243b5e05" /><zone lrx="1282" lry="1262" ulx="1139" uly="1233" xml:id="zone_d3131fa2-c397-11e8-b6d8-f40f243b5e05" /><zone lrx="1266" lry="1262" ulx="1258" uly="1233" xml:id="zone_d313205c-c397-11e8-9226-f40f243b5e05" /><zone lrx="1314" lry="1262" ulx="1271" uly="1233" xml:id="zone_d3132112-c397-11e8-babb-f40f243b5e05" /><zone lrx="1237" lry="1289" ulx="1191" uly="1263" xml:id="zone_d31321c6-c397-11e8-864c-f40f243b5e05" /><zone lrx="1310" lry="1286" ulx="1303" uly="1262" xml:id="zone_d313227a-c397-11e8-848d-f40f243b5e05" /><zone lrx="1380" lry="1353" ulx="1140" uly="1322" xml:id="zone_d313232e-c397-11e8-8890-f40f243b5e05" /><zone lrx="1431" lry="1412" ulx="1127" uly="1379" xml:id="zone_d313243e-c397-11e8-8e85-f40f243b5e05" /><zone lrx="1118" lry="1514" ulx="1089" uly="1506" xml:id="zone_d3132518-c397-11e8-9bc9-f40f243b5e05" /><zone lrx="1118" lry="1538" ulx="1089" uly="1530" xml:id="zone_d31325e2-c397-11e8-9715-f40f243b5e05" /><zone lrx="1170" lry="1735" ulx="1163" uly="1712" xml:id="zone_d313269e-c397-11e8-ba77-f40f243b5e05" /><zone lrx="1292" lry="1739" ulx="1211" uly="1713" xml:id="zone_d3132764-c397-11e8-8b91-f40f243b5e05" /></surface><surface><graphic n="https://iiif.dl.itc.u-tokyo.ac.jp/repo/iiif/112931/canvas/p19" url="https://iiif.dl.itc.u-tokyo.ac.jp/repo/files/original/112931/94030101-019_001.jpg" /></surface><surface><graphic n="https://iiif.dl.itc.u-tokyo.ac.jp/repo/iiif/112931/canvas/p20" url="https://iiif.dl.itc.u-tokyo.ac.jp/repo/files/original/112931/94030101-020_001.jpg" /></surface><surface><graphic n="https://iiif.dl.itc.u-tokyo.ac.jp/repo/iiif/112931/canvas/p21" url="https://iiif.dl.itc.u-tokyo.ac.jp/repo/files/original/112931/94030101-021_001.jpg" /></surface><surface><graphic n="https://iiif.dl.itc.u-tokyo.ac.jp/repo/iiif/112931/canvas/p22" url="https://iiif.dl.itc.u-tokyo.ac.jp/repo/files/original/112931/94030101-022_001.jpg" /><zone lrx="2163" lry="1084" ulx="1687" uly="341" xml:id="zone_d3132c6e-c397-11e8-8792-f40f243b5e05" /></surface></surfaceGrp>
